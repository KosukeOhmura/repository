  <table class="table table-hover table-striped">
    <thead>
      <tr>
        <th>ユーザー</th>
        <th>管理者</th>
      </tr>
    </thead>
    <tbody>
      <% @project.project_users.each do |project_user| %>
        <% if project_user.deleted_at.nil? %>
          <tr>
            <td>
              <%= project_user.user.nm %>
            </td>
            <td><%= project_user.admin_flag ? "管理者" : "" %></td>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>

<%= render partial: "temps/combo", locals: {} %>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>論理名</th>
      <th>物理名</th>
      <th>コード</th>
      <th>編集</th>
      <th>削除</th>
    </tr>
  </thead>

  <tbody>
    <% @kbns.each do |kbn| %>
      <tr>
        <td><%= link_to kbn.logical_name, project_kbn_path(@project, kbn) %></td>
        <td><%= kbn.physical_name %></td>
        <td><%= kbn.code %></td>
        <td><%= link_to 'Edit', edit_project_kbn_path(@project, kbn) %></td>
        <td><%= link_to 'Destroy', project_kbn_path(@project, kbn), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br/>

<%= link_to '新規作成', new_project_kbn_path(@project), class: 'btn btn-default' %>
